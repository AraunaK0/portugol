programa {
  funcao inicio() {
    inteiro a, b, soma, multiplicacao

    escreva("Digite o valor A: ")
    leia(a)
    escreva("Digite o valor B: ")
    leia(b)

    se (a == b){
      soma = a + b
      escreva("\nVariável C: " + soma)

    } 
    senao {
      multiplicacao = a * b
      escreva("\nVariável C: "  + multiplicacao)

    }



    
  }
}
